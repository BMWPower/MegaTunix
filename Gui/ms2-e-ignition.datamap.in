raw_upper=1800
dl_conv_expr=x/0.1
ul_conv_expr=x*0.1
precision=1
bind_to_list=trigger_return
_tooltip=See http://msextra.com/doc/ms2extra/MS2-Extra_Gen_Ignition.htm


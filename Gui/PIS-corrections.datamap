#----- CTS Settings / Corrections ---------------------------------------------

[CTS_bin]
keys=page,dl_conv_expr,ul_conv_expr,dl_type,handler,raw_lower,raw_upper,size,use_color,tooltip,temp_dep,bind_to_list
page=0
offset=414
dl_type=_IMMEDIATE_
handler=_GENERIC_
size=_U16_
raw_lower=0
raw_upper=1000
dl_conv_expr=(x+459.67)*(5/9)
ul_conv_expr=x*(9/5)-459.67
use_color=TRUE
tooltip=The CTS bins are used to determine what corrections or settings are to be applied according to the Coolant Temperature Sensor. NOTE: The ECU does NOT extrapolate beyond the edge of the table. The values are clamped at the borders.  Editing is accomplished by either +/-, PgUp/PgDn, or by typing in a value over the existing value.  The value will turn red during an edit and WILL NOT BE SENT until you hit enter.  Hitting escape will cancel an edit and return to the previous value.
temp_dep=TRUE
bind_to_list=temperature

[CPW]

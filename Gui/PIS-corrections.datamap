[global]
description=Object data map for alpha-n base ignition +advance / -retard tab
tab_name=_Corrections
id_prefix=COR_
groups=defaults

#----- Groups -----------------------------------------------------------------

[defaults]
keys=page,size
size=_U16_
page=0

#----- topframe  --------------------------------------------------------------

[topframe]
keys=tab_ident,sub-notebook,bind_to_list,table_num
tab_ident=_CORRECTIONS_TAB_
sub-notebook=table_notebook
bind_to_list=valid_ecu_ctrls
table_num=0

#----- Buttons ----------------------------------------------------------------

[CTS_bin_edit]
keys=te_table_num,handler
te_table_num=0
handler=_TE_TABLE_

[CTS_asc_precent_edit]
keys=te_table_num,handler
te_table_num=1
handler=_TE_TABLE_

[CTS_asc_duration_edit]
keys=te_table_num,handler
te_table_num=2
handler=_TE_TABLE_

[CTS_limit_edit]
keys=te_table_num,handler
te_table_num=3
handler=_TE_TABLE_

[CTS_bpw_edit]
keys=te_table_num,handler
te_table_num=4
handler=_TE_TABLE_

[CTS_iar_edit]
keys=te_table_num,handler
te_table_num=5
handler=_TE_TABLE_

[CTS_ae_edit]
keys=te_table_num,handler
te_table_num=6
handler=_TE_TABLE_

[CTS_group_edit]
keys=handler,te_tables
handler=_TE_TABLE_GROUP_
te_tables=0,1,2,3,4,5,6

[MAT_bpw_edit]
keys=te_table_num,handler
te_table_num=7
handler=_TE_TABLE_

[MAT_iar_edit]
keys=te_table_num,handler
te_table_num=8
handler=_TE_TABLE_

[MAT_group_edit]
keys=handler,te_tables
handler=_TE_TABLE_GROUP_
te_tables=7,8

[open_delay_edit]
keys=te_table_num,handler
te_table_num=9
handler=_TE_TABLE_

[dwell_edit]
keys=te_table_num,handler
te_table_num=10
handler=_TE_TABLE_

[voltage_group_edit]
keys=handler,te_tables
handler=_TE_TABLE_GROUP_
te_tables=9,10

[runtime_start_realtime_button]
keys=handler
handler=_START_REALTIME_

[runtime_stop_realtime_button]
keys=handler
handler=_STOP_REALTIME_

[get_data_button]
keys=handler,bind_to_list,tooltip
handler=_READ_VE_CONST_
bind_to_list=get_data_buttons
tooltip=This button retrieves the COR constants block of data and updates all controls with the current values.

[burn_data_button]
keys=handler,bind_to_list,tooltip
handler=_BURN_FLASH_
bind_to_list=burners
tooltip=This button will turn red when a value is changed in the ECU's RAM but it needs to be burnt to flash for permanent storage.  By clicking this it will burn the ram to flash and the button will turn back to black.


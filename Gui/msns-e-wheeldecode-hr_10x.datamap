[trig_wheel_teeth_entry]
keys=offset,tooltip
offset=90
tooltip=The total number of teeth including the "missing" ones.  I.e a 36-1 tooth wheel has spaces for 36 evenly spaced teeth, even though 1 is missing, so it technically has 35 teeth,  but you would enter "36" in this field, as the wheel is spaced out for 36 teeth.

[trigger_angle_entry]
keys=page,offset,precision,dl_conv_expr,ul_conv_expr,handler,spconfig_offset,tooltip
page=3
offset=168
precision=2
dl_conv_expr=x*(284/100)
ul_conv_expr=x/(284/100)
handler=_TRIGGER_ANGLE_
spconfig_offset=173
tooltip=This the amount of degree's BTDC that the triggers come at. i.e. to allow the maxiumum timing advance you want the trigger to arrive before your maximum spark advance plus a cushion of 10-20 deg. So if you max advance is 40 deg, a trigger angle of 60deg is safe.  NOTE: this differs somewhat from megatune which has only a 0-90 deg angle range of this control and an additional bitfield.  MegaTunix handles this cleanly with one control with a 0-135 deg range.

[crank_angle_entry]
keys=page,offset,precision,dl_conv_expr,ul_conv_expr,tooltip
page=3
offset=171
precision=2
dl_conv_expr=(x/(352/1000))+(284/10)
ul_conv_expr=(x-(284/10))*(352/1000)
tooltip=This is the Crank Timing (when not using trigger return)

[fixed_angle_entry]
keys=page,offset,precision,dl_conv_expr,ul_conv_expr,tooltip
page=3
offset=169
precision=2
dl_conv_expr=(x/(352/1000))+(284/10)
ul_conv_expr=(x-(284/10))*(352/1000)
tooltip=This control locks out the Spark table and holds the timing ridgly at this value. Setting this to -10 UNLOCKS the timing control and lets it use the spark tables.

[trim_angle_entry]
keys=page,offset,precision,dl_conv_expr,ul_conv_expr,tooltip
page=3
offset=170
precision=2
dl_conv_expr=x/(352/1000)
ul_conv_expr=x*(352/1000)
tooltip=This is global offset for the crank table, to shift all values one way or another.

[ign_hold_entry]
keys=page,offset,raw_upper,tooltip
page=3
offset=172
raw_upper=100
tooltip=Holds off ignition for x number of triggers to allow engine RPM to stabilize a bit when cranking just starts.  Helps eliminiate screwy initial spark timing

[crankdwell_entry]
keys=offset,precision,dl_conv_expr,ul_conv_expr,tooltip
offset=110
precision=1
dl_conv_expr=x*10
ul_conv_expr=x/10
tooltip=This is the ignition dwell during cranking. 

[rundwell_entry]
keys=offset,precision,dl_conv_expr,ul_conv_expr,raw_upper,tooltip
offset=111
precision=1
dl_conv_expr=x*10
ul_conv_expr=x/10
raw_upper=80
tooltip=This is the ignition dwell during normal engine running. 

[min_discharge_entry]
keys=offset,precision,raw_upper,dl_conv_expr,ul_conv_expr,tooltip
offset=143
precision=1
raw_upper=15
dl_conv_expr=x*10
ul_conv_expr=x/10
tooltip=This is the time allowed between ignition events for the plug to fire before the coil is allowed to recharge. This can limit dwell at high RPM's.

[s4w_num_steps_entry]

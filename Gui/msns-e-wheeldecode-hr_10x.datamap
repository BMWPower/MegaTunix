choices=Disabled,Enabled
bitvals=0,1
offset=92
bitmask=16
tooltip=Use dual distributor mode or not

[invert_ign_combo]
keys=page,offset,choices,bitvals,bitmask,tooltip
page=3
choices=Normal,Inverted
bitvals=0,1
offset=173
bitmask=8
tooltip=Sets ignition output polarity

[crank_trig_combo]
keys=page,offset,choices,bitvals,bitmask,toggle_groups,bind_to_list,tooltip
page=3
choices=Trigger Return,Time Based
toggle_groups=trigger_return,time_based
bind_to_list=nonedis_controls,spark_controls
bitvals=0,1
offset=173
bitmask=4
tooltip=Sets the cranking timing control to use Trigger Return (second column of Trigger points table above) or time based timing (which can be error prone if the engine doesn't have a stable and consistent cranking RPM)

[s4w_combo]

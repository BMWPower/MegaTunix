#----- Widgets ---------------------------------------------------------------

[s4w_eventbox]
keys=tooltip,bind_to_list
bind_to_list=wheel_decode
tooltip=Stim for wheel is ONLY FOR USE on the stimulator. DO NOT USE THIS MODE on the car.  This option is for toothed wheel users to test their Megasquirt on the bench using the stimulator where the correct input signal is unavailable. DO NOT use on the real engine or you will flood the engine with fuel.

[fixed_dwell_table]
keys=page,bind_to_list
page=7
bind_to_list=fixed_dwell

[dwell_params_table]
keys=page,bind_to_list
page=7
bind_to_list=dwell_control

[triggers_frame]
keys=page,bind_to_list
page=7
bind_to_list=wheel_decode

[wheel_params_frame]
keys=page,bind_to_list
page=7
bind_to_list=wheel_decode

[wheel_hbox1]
keys=page,bind_to_list
page=7
bind_to_list=spark_controls

#----- Buttons ---------------------------------------------------------------

[get_data_button]
keys=handler,page,bind_to_list,tooltip
handler=_READ_VE_CONST_
page=7
bind_to_list=get_data_buttons
tooltip=This button retrieves the VE/Constants block of data (all pages) and updates all controls with the current values.

[burn_data_button]
keys=handler,page,bind_to_list,tooltip
handler=_BURN_MS_FLASH_
page=7
bind_to_list=burners
tooltip=This button will turn red when a value is changed in the ECU's RAM but it needs to be burnt to flash for permanent storage.  By clicking this it will burn the ram to flash and the button will turn back to black.


#----- Radio Buttons ----------------------------------------------------------

[decoder_combo]
keys=offset,choices,bitvals,bitmask,toggle_groups,tooltip
choices=025 Style,024s9 Style
toggle_groups=,025_spark
bitvals=0,1
offset=92
bitmask=8
tooltip=Sets the wheel decode algorithm to either 025 style or 024s9 style.

[style_combo]
keys=offset,choices,bitvals,bitmask,tooltip
choices=-1,-2
bitvals=0,1
offset=0
bitmask=8
tooltip=Sets the wheel to be of the "-1" variety, (i.e. 36-1, 12-1, 4-1, etc) or of the "-2" variety, (i.e. 60-2)

[2nd_trig_combo]
keys=offset,choices,bitvals,bitmask,toggle_groups,tooltip
choices=Disabled,Enabled
toggle_groups=,2nd_trigger
bitvals=0,1
offset=11
bitmask=1
tooltip=Sets the second trigger input

[2nd_trig_edge_combo]
keys=offset,choices,bitvals,bitmask,bind_to_list,tooltip
choices=Rising Edge,Falling Edge,Both Edges
bind_to_list=2nd_trigger
bitvals=0,1,2
offset=33
bitmask=3
tooltip=Sets the second trigger input to trigger on the Rising or Falling Edge.

[2nd_trig_missing_combo]

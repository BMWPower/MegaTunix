

[global]
description=Object data map for Advanced settings 
tab_name=_Advanced Options
id_prefix=adv_
groups=defaults

#----- Groups ----------------------------------------------------------------

[defaults]
keys=page,size,raw_upper
key_types=_INT_,_ENUM_,_INT_

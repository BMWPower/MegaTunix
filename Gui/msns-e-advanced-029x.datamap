offset=80
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=This sets the threshold point at which to enable injector staging. This is a raw units control, and thus is either "RPM/100", "KPa" or "TPSADC" as selected by the above buttons.

[staging_off_delta_entry]
keys=page,offset,dl_type,handler,tooltip

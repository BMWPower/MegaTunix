[global]
description=Object data map for the MS-2 Globals tab
tab_name=_Global Settings
id_prefix=MS2_GLOB_
groups=defaults

#----- Groups ----------------------------------------------------------------

[defaults]
keys=page,size,raw_lower,raw_upper,dl_type,handler
page=0
size=_U08_
raw_lower=0
raw_upper=255
dl_type=_IMMEDIATE_
handler=_GENERIC_

#----- Labels ---------------------------------------------------------------

[num_injectors_0_label]
keys=page,bind_to_list
page=1
bind_to_list=interdep_0_ctrl

[num_squirts_per_cycle_0_label]
keys=page,bind_to_list
page=1
bind_to_list=interdep_0_ctrl

[num_cylinders_0_label]
keys=page,bind_to_list
page=2
bind_to_list=interdep_0_ctrl

[2nd_fuelload_label]
keys=bind_to_list
bind_to_list=single_table

[secondary_fuel_label]
keys=bind_to_list
bind_to_list=single_table

[realtime_baro_port_label]
keys=bind_to_list
bind_to_list=baro_dual

#----- Widgets ---------------------------------------------------------------

[inj2_properties_table]
keys=bind_to_list
bind_to_list=bank2

[incorp_afr_param_table]
keys=bind_to_list
bind_to_list=incorp_afrtarget

[def_baro_table]
keys=bind_to_list
bind_to_list=def_baro

[low_baro_table]
keys=bind_to_list
bind_to_list=low_baro

[high_baro_table]
keys=bind_to_list
bind_to_list=high_baro

#----- Buttons ---------------------------------------------------------------

[get_data_button]
keys=handler,bind_to_list,tooltip
handler=_READ_VE_CONST_
bind_to_list=get_data_buttons
tooltip=This button retrieves the VE/Constants block of data (all pages) and updates all controls with the current values.

[burn_data_button]
keys=handler,bind_to_list,tooltip
handler=_BURN_MS_FLASH_
bind_to_list=burners
tooltip=This button will turn red when a value is changed in the ECU's RAM but it needs to be burnt to flash for permanent storage.  By clicking this it will burn the ram to flash and the button will turn back to black.

[calc_reqfuel_button]
keys=handler,tooltip,table_num
handler=_REQ_FUEL_POPUP_
table_num=3
tooltip=This button Pops up a window to calculate the required fuel



#----- Spinbuttons -----------------------------------------------------------

[req_fuel_per_cycle_0_spin]
keys=offset,size,table_num,dl_type,handler,bind_to_list,ul_complex,ul_conv_expr,register_as
offset=608
size=_U16_
table_num=3
dl_type=_DEFERRED_
handler=_REQ_FUEL_1_
bind_to_list=interdep_0_ctrl
ul_complex=TRUE
ul_conv_expr=((reqfuel_1)*((injectors_1)/(divider_1*(alternate+1))))/1000
expr_symbols=reqfuel_1,injectors_1,divider_1,alternate
expr_types=_VE_VAR_,_VE_EMB_BIT_,_VE_VAR_,_VE_EMB_BIT_
reqfuel_1_page=0
reqfuel_1_offset=608
reqfuel_1_size=_U16_
injectors_1_page=0
injectors_1_offset=619
injectors_1_bitmask=15
divider_1_page=0
divider_1_offset=610
alternate_page=0
alternate_offset=611
alternate_bitmask=1
register_as=req_fuel_per_cycle_0_spin

[req_fuel_per_squirt_0_spin]
keys=offset,size,table_num,ul_conv_expr,bind_to_list,register_as
offset=608
size=_U16_
table_num=3
ul_conv_expr=x/1000
bind_to_list=interdep_0_ctrl
register_as=req_fuel_per_squirt_0_spin

[num_injectors_0_spin]
keys=offset,bitmask,table_num,dl_type,handler,bind_to_list,register_as
offset=619
bitmask=15
table_num=3
dl_type=_DEFERRED_
handler=_NUM_INJECTORS_1_
bind_to_list=interdep_0_ctrl
register_as=num_injectors_0_spin

[num_cylinders_0_spin]
keys=offset,bitmask,table_num,dl_type,handler,bind_to_list,register_as
offset=0
bitmask=15
table_num=3
dl_type=_DEFERRED_
handler=_NUM_CYLINDERS_1_
bind_to_list=interdep_0_ctrl
register_as=num_cylinders_0_spin

[inj_per_cycle_0_spin]
keys=offset,table_num,dl_type,handler,bind_to_list,ul_complex,ul_conv_expr,register_as
offset=610
table_num=3
dl_type=_DEFERRED_
handler=_NUM_SQUIRTS_1_
bind_to_list=interdep_0_ctrl
ul_complex=TRUE
ul_conv_expr=cylinders_1/divider_1
expr_symbols=cylinders_1,divider_1
expr_types=_VE_EMB_BIT_,_VE_VAR_
cylinders_1_page=0
cylinders_1_offset=0
cylinders_1_bitmask=15
divider_1_page=0
divider_1_offset=610
register_as=inj_per_cycle_0_spin

#----- Entries ---------------------------------------------------------------

[stoich_afr_entry]
keys=offset,size,precision,dl_conv_expr,ul_conv_expr,raw_upper,tooltip
offset=662
size=_S16_
dl_conv_expr=x*10
ul_conv_expr=x/10
raw_upper=255
precision=1
tooltip=This is the stoich AFR

[inj1_open_time_entry]
keys=offset,size,precision,dl_conv_expr,ul_conv_expr,raw_upper,tooltip
offset=566
size=_U16_
dl_conv_expr=x/0.001
ul_conv_expr=x*0.001
raw_upper=25000
precision=3
tooltip=This is the amount of time the injector spends in the opening or closing phase.  It's used to tweake the fuel equations to compensate for the fact that fuel flow doesn't go from 0 to max instantaneously

[inj1_batt_corr_entry]
keys=offset,precision,size,dl_conv_expr,ul_conv_expr,tooltip
offset=568
precision=3
size=_U16_
dl_conv_expr=x/0.000166667
ul_conv_expr=x*0.000166667
tooltip=This compensates for battery voltage, at lower voltages the injectors open slower, skewing fuel flow slightly. this corrects for that.

[inj1_pwm_limit_entry]
keys=offset,raw_upper,tooltip
offset=615
raw_upper=100
tooltip=This sets the PWM current limit DutyCycle %, 100 being no limit.


# Bowling/Grippo Constants For most MS versions EXCEPT DT and MSnS-Extra

[global]
description=Object data map for B&G Constants (most firmwares except DT... )
tab_name=_Engine Vitals
groups=defaults

#----- Groups ----------------------------------------------------------------

[defaults]
keys=page,size
key_types=_INT_,_ENUM_
page=0
size=_U08_

#----- Buttons ---------------------------------------------------------------

[bng_get_data_button]
keys=handler,page,bind_to_list
key_types=_ENUM_,_INT_,_STRING_
handler=_READ_VE_CONST_
page=0
bind_to_list=get_data_buttons

[bng_burn_data_button]
keys=handler,page,bind_to_list
key_types=_ENUM_,_INT_,_STRING_
handler=_BURN_MS_FLASH_
page=0
bind_to_list=burners

[calc_req_fuel_button]
keys=page,handler,table_num
key_types=_INT_,_ENUM_,_STRING_
page=0
table_num=0
handler=_REQ_FUEL_POPUP_

#----- Labels ----------------------------------------------------------------

[fast_idle_thresh_temp_label]
keys=page,c_label,f_label,temp_dep,bind_to_list
key_types=_INT_,_STRING_,_STRING_,_BOOL_,_STRING_
page=0
c_label=Fast Idle Threshold (\302\260C.)
f_label=Fast Idle Threshold (\302\260F.)
temp_dep=TRUE
bind_to_list=temperature

[num_injectors_1_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=interdep_1_ctrl

[num_squirts_per_cycle_1_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=interdep_1_ctrl,reqfuel_1_ctrl

[num_cylinders_1_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=interdep_1_ctrl,reqfuel_1_ctrl


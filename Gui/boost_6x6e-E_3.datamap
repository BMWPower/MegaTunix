key_types=_INT_,_STRING_
page=7
set_label=Boost Table 3 (KPA Target)

[boost_filler_label]
keys=page,set_label
key_types=_INT_,_STRING_
page=7
set_label=KPA

[boost_label]
keys=page,set_label

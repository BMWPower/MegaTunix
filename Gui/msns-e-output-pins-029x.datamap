# MSnS-Extra Output Pins tab

[global]
description=Object data map for MSnS-Extra Output Pins
tab_name=_Output Pins
id_prefix=output_pins_

#----- Buttons ---------------------------------------------------------------

[get_data_button]
keys=handler,page,bind_to_list
key_types=_ENUM_,_INT_,_STRING_
handler=_READ_VE_CONST_
page=0
bind_to_list=get_data_buttons

[burn_data_button]
keys=handler,page,bind_to_list
key_types=_ENUM_,_INT_,_STRING_
handler=_BURN_MS_FLASH_
page=0
bind_to_list=burners

#------ Labels --------------------------------------------------------------
[shiftlight_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=shiftlight_controller

[x4_output_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=x4_output_controls

[shift_low_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=shiftlight_controller,shift_controls

[shift_high_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=shiftlight_controller,shift_controls

[tach_output_rate_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=tachout_params

[efan_on_temp_label]
keys=page,bind_to_list,temp_dep,c_label,f_label
key_types=_INT_,_STRING_,_BOOL_,_STRING_,_STRING_
page=0
temp_dep=TRUE
c_label=Fan On Temp \302\260C.
f_label=Fan On Temp \302\260F.
bind_to_list=fan_controls,temperature

[efan_off_temp_label]
keys=page,bind_to_list,temp_dep,c_label,f_label
key_types=_INT_,_STRING_,_BOOL_,_STRING_,_STRING_
page=0
temp_dep=TRUE
c_label=Fan Off Temp \302\260C.
f_label=Fan Off Temp \302\260F.
bind_to_list=fan_controls,temperature


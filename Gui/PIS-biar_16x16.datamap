[global]
description=Object data map for base ignition +advance / -retard tab
tab_name=Base _Ignition
id_prefix=BIAR_
groups=defaults

#----- Groups ----------------------------------------------------------------

[defaults]
keys=page,size
size=_U16_
page=0

#----- topframe & title ----------------------------------------------------------------

[topframe]
keys=table_num,tab_ident,sub-notebook,bind_to_list
tab_ident=_SPARKTABLES_TAB_
sub-notebook=table_notebook
bind_to_list=valid_ecu_ctrls
table_num=1

#----- Buttons ---------------------------------------------------------------

[runtime_start_realtime_button]
keys=handler,page
handler=_START_REALTIME_
page=0

[runtime_stop_realtime_button]
keys=handler,page
handler=_STOP_REALTIME_
page=0

[get_data_button]
keys=handler,bind_to_list,tooltip
handler=_READ_VE_CONST_
bind_to_list=get_data_buttons
tooltip=This button retrieves the Base Ignition +Advance / -Retards (BIAR) constants block of data and updates all controls with the current values.

[burn_data_button]
keys=handler,bind_to_list,tooltip
handler=_BURN_FLASH_
bind_to_list=burners
tooltip=This button will turn red when a value is changed in the ECU's RAM but it needs to be burnt to flash for permanent storage.  By clicking this it will burn the ram to flash and the button will turn back to black.

[BIAR_tracking_checkbutton]
keys=handler,table_num
handler=_TRACKING_FOCUS_
table_num=1

[BIAR_export_button]
keys=handler,table_num
handler=_EXPORT_SINGLE_TABLE_
table_num=1

[BIAR_import_button]
keys=handler,table_num
handler=_IMPORT_SINGLE_TABLE_
table_num=1

[BIAR_3d_view_button]
keys=table_num,tooltip,bind_to_list
table_num=1
bind_to_list=3d_buttons
tooltip=This pops up a 3 dimensional view of the current Base Ignition +Advance / -Retard Table.  Editing of the table in the 3D view is supported with the +/- and PgUp/PgDn keys (fine/coarse edit) and the arrows navigate. A Red dot shows the current edit position as does the indicators on the right side of the window beneath the buttons.  A left mouse button hold down and drag of the mouse in the 3D view will rotate the display.  Using the middle mouse button will strafe (slide) and the right mouse button will zoom.

[BIAR_rescale_button]
keys=handler,data
handler=_RESCALE_TABLE_
data=B_VE1_rescale_entry,VE_VE1_rescale_combo

#----- Widgets --------------------------------------------------------------

[BIAR_vbox1]
keys=table_num
table_num=1

#----- Gauges --------------------------------------------------------------

[BIAR_gaugeframe1]
keys=post_functions_with_arg,gaugexml,datasource,table_num
post_functions_with_arg=create_gauge
gaugexml=SportComp/AM_SportComp_RPM_Gauge.xml
datasource=status_rpm_rpm
table_num=1

[BIAR_gaugeframe2]
keys=post_functions_with_arg,gaugexml,datasource,table_num
post_functions_with_arg=create_gauge
gaugexml=SportComp/AM_SportComp_MAP_Gauge.xml
datasource=status_map_map
table_num=1

[BIAR_gaugeframe3]
keys=post_functions_with_arg,gaugexml,datasource,table_num
post_functions_with_arg=create_gauge
gaugexml=SportComp/AM_SportComp_CLT_degC_Gauge.xml
datasource=cts_temp
table_num=1

[BIAR_gaugeframe4]
keys=post_functions_with_arg,gaugexml,datasource,table_num
post_functions_with_arg=create_gauge
gaugexml=SportComp/AM_SportComp_Advance_Gauge.xml
datasource=status_ign_AIAR
table_num=1

[BIAR_gaugeframe5]
keys=post_functions_with_arg,gaugexml,datasource,table_num
post_functions_with_arg=create_gauge
gaugexml=SportComp/AM_SportComp_O2Lambda_Gauge.xml
datasource=lambda
table_num=1

#----- Rescale Entry ------------------------------------------------------

[BIAR_rescale_entry]
keys=table_num
table_num=1

#----- Rescale Combo ------------------------------------------------------

[BIAR_rescale_combo]
keys=table_num,post_functions_with_arg
table_num=1
post_functions_with_arg=lock_entry

#----- MAIN PWTable Container ---------------------------------------------

[BIAR_main_table]
keys=rescaler_frame
rescaler_frame=BIAR_rescale_frame

#----- Load Entries (MAP/TPS) -----------------------------------------------

[BIAR_load_entry]
keys=page,dl_type,handler,raw_lower,raw_upper,size,dl_conv_expr,ul_conv_expr,use_color,tooltip
page=0
offset=38
dl_type=_IMMEDIATE_
handler=_GENERIC_
size=_U16_
raw_lower=0
raw_upper=350
dl_conv_expr=x/1
ul_conv_expr=x*1
use_color=TRUE
tooltip=The Manifold Absolute Pressure (MAP) bins in combination with the RPM bins on the bottom of the Base Ignition +Advance / -Retards (BIAR) table define the vertexes (points on the BIAR map).  The ECU will interpolate between vertexes providing a smooth transition between vertexes on the map.  The 3D view shows this graphically.  NOTE: The ECU does NOT extrapolate beyond the edge of the table. The values are clamped at the borders.  Editing is accomplished by either +/-, PgUp/PgDn, or by typing in a value over the existing value.  The value will turn red during an edit and WILL NOT BE SENT until you hit enter.  Hitting escape will cancel an edit and return to the previous value.

#----- RPM Entries ---------------------------------------------------------

[BIAR_rpm_entry]
keys=page,dl_conv_expr,ul_conv_expr,dl_type,handler,raw_lower,raw_upper,size,use_color,tooltip
page=0
offset=6
dl_conv_expr=x/1
ul_conv_expr=x*1
size=_U16_
dl_type=_IMMEDIATE_
handler=_GENERIC_
raw_lower=0
raw_upper=16384
use_color=TRUE
tooltip=The RPM bins in combination with the Manifold Absolute Pressure (MAP) bins on the left side of the Base Ignition +Advance / -Retards (BIAR) table define the vertexes (points on the BIAR map).  The ECU will interpolate between vertexes providing a smooth transition between points on the BIAR map. The 3D view shows this graphically.  NOTE: the ECU does NOT extrapolate beyond the edge of the table. The values are clamped at the borders.  Editing is accomplished by either +/-, PgUp/PgDn, or by typing in a value over the existing value.  The value will turn red during an edit and WILL NOT BE SENT until you hit enter.  Hitting escape will cancel an edit and return to the previous value.

#----- BIAR-Table Entries -----------------------------------------------------

[BIAR_entry]
keys=page,dl_type,handler,raw_lower,raw_upper,use_color,tooltip,size,ul_conv_expr,dl_conv_expr,precision
page=0
offset=2164
dl_type=_IMMEDIATE_
handler=_GENERIC_
raw_lower=-30
raw_upper=255
size=_S16_
dl_conv_expr=x*(28444/10000)
ul_conv_expr=x/(28444/10000)
precision=1
use_color=TRUE
tooltip=The Base Ignition +Advance / -Retard (BIAR) Entries control the ignition timing. Editing is accomplished by either +/-, PgUp/PgDn, or by typing in a value over the existing value.  The value will turn red during an edit and WILL NOT BE SENT until you hit enter.  Hitting escape will cancel an edit and return to the previous value.


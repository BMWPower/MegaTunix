[global]
description=Object data map for WaterInj/NOS/LaunchCtrl for msns-E
tab_name=_Water/NOS/Launch
id_prefix=wnl_
groups=defaults

#----- Groups ----------------------------------------------------------------

[defaults]
keys=page,size,raw_lower,raw_upper,dl_type,handler
key_types=_INT_,_ENUM_,_INT_,_INT_,_ENUM_,_ENUM_
page=0
size=_U08_
raw_lower=0
raw_upper=255
dl_type=_IMMEDIATE_
handler=_GENERIC_

#----- Buttons ---------------------------------------------------------------

[get_data_button]
keys=handler,bind_to_list
key_types=_ENUM_,_STRING_
handler=_READ_VE_CONST_
bind_to_list=get_data_buttons

[burn_data_button]
keys=handler,bind_to_list
key_types=_ENUM_,_STRING_
handler=_BURN_MS_FLASH_
bind_to_list=burners

#----- Widgets ----------------------------------------------------------------

[nitrous_frame]
keys=bind_to_list
key_types=_STRING_
bind_to_list=nos_features,x2_nitrous_control,x3_nitrous_control

[water_inj_frame]
keys=bind_to_list
key_types=_STRING_
bind_to_list=x2_water_injection,x3_water_injection

[flat_shift_frame]
keys=bind_to_list
key_types=_STRING_
bind_to_list=launch_controls

[water_inj_params_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=water_controls

[water_inj_features_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=water_features

[nitrous_feature_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=nos_features

[nos_params_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=nos_params

[nos_dt_mapping_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=dt_controls,nos_params

[nos_antilag_feature_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=nos_params

[nos_antilag_params_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=antilag_controls,nos_params

[launch_params_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=launch_controls

[variable_launch_feature_table]
keys=bind_to_list
key_types=_STRING_
bind_to_list=launch_controls

[launch_feature_ebox]
keys=tooltip
key_types=_STRING_
tooltip=Launch control is used to limit the rpm of the engine for rapid launch (drag racing), By enabling this you can select how to limit the engine's power for a faster takeoff.  NOTE: spark drop is NOT available on EDIS setups, and the soft limiter is NOT available at all when NOT using a spark mode.

[variable_launch_feature_ebox]
keys=tooltip
key_types=_STRING_
tooltip=Variable Launch control uses an external input for the user to set the hard cut limit without needed to run any tuning software.  The Best way to implement it is to hada thumbswitch that is easily accessed, rev the engine to the target hardlimit rpm and hit the button.  The ECU will use this rpm as the hard limit RPM. See http://megasquirt.sf.net/extra/launch.htm

#----- Labels ----------------------------------------------------------------

[lc_sparkdrop_label]
keys=bind_to_list
key_types=_STRING_
bind_to_list=launch_controls,lc_spark_ctrls,nonedis_controls

[lc_sparkbase_label]
keys=bind_to_list
key_types=_STRING_
bind_to_list=launch_controls,lc_spark_ctrls,nonedis_controls

[nitrous_frame_title]
keys=bind_to_list
key_types=_STRING_
bind_to_list=nos_features,x2_nitrous_control,x3_nitrous_control

[water_inj_frame_title]
keys=bind_to_list
key_types=_STRING_
bind_to_list=water_features,x2_water_injection,x3_water_injection

[nos_spark_retard_label]
keys=bind_to_list
key_types=_STRING_
bind_to_list=spark_controls,x2_nitrous_control,x3_nitrous_control

[nos_delay_after_launch_label]
keys=bind_to_list
key_types=_STRING_
bind_to_list=launch_controls,nos_params

[nos_delay_after_flat_shift_label]
keys=bind_to_list
key_types=_STRING_
bind_to_list=launch_controls,nos_params

[water_iat_setpoint_label]
keys=c_label,f_label,temp_dep,bind_to_list
key_types=_STRING_,_STRING_,_BOOL_,_STRING_

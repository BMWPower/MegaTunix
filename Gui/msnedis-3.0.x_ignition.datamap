# MegaSquirtnEDIS 3.0X ignition parameters tab

[global]
description=Object data map for MSnEDIS-3.0.x ignition params
tab_name=_Ignition Params


#----- Buttons ---------------------------------------------------------------
[ign_get_data_button]
keys=handler,page
key_types=_ENUM_,_INT_
handler=_READ_VE_CONST_
page=0

[ign_burn_button]
keys=handler,page,bind_to_list
key_types=_ENUM_,_INT_,_STRING_
handler=_BURN_MS_FLASH_
page=0
bind_to_list=burners

#----- Dynamic Labels (temp dependant) ---------------------------------------


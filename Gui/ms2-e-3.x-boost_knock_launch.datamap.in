bind_to_list=knock_remote,can_polling,can_poll_ports,poll_generic,can_pollers

[obp_basics_table]
keys=bind_to_list
bind_to_list=obp_fuel

[obp_spark_table]
keys=bind_to_list
bind_to_list=obp_spark

[launch_left_table]
keys=bind_to_list
bind_to_list=launch_control

[launch_right_params_table]

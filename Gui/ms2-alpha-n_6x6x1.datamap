#----- Widgets ----------------------------------------------------------------

[AN_vbox1]
keys=table_num
key_types=_STRING_
table_num=5

#----- Labels ----------------------------------------------------------------

[topframe]
keys=tab_ident,table_num,bind_to_list
key_types=_ENUM_,_STRING_,_STRING_
tab_ident=_ALPHA_N_TAB_
bind_to_list=an_ctrls
table_num=5

#----- widgets ---------------------------------------------------------------


#----- Buttons ---------------------------------------------------------------

[get_data_button]
keys=handler,bind_to_list,tooltip
key_types=_ENUM_,_STRING_,_STRING_
handler=_READ_VE_CONST_
bind_to_list=get_data_buttons
tooltip=This button retrieves the VE/Constants block of data (all pages) and updates all controls with the current values.

[burn_data_button]
keys=handler,bind_to_list,tooltip
key_types=_ENUM_,_STRING_,_STRING_
handler=_BURN_MS_FLASH_
bind_to_list=burners
tooltip=This button will turn red when a value is changed in the ECU's RAM but it needs to be burnt to flash for permanent storage.  By clicking this it will burn the ram to flash and the button will turn back to black.

[AN_tracking_checkbutton]
keys=handler,table_num
key_types=_ENUM_,_STRING_
handler=_TRACKING_FOCUS_
table_num=5

[AN_export_button]
keys=handler,table_num
key_types=_ENUM_,_STRING_
handler=_EXPORT_SINGLE_TABLE_
table_num=5

[AN_import_button]
keys=handler,table_num
key_types=_ENUM_,_STRING_
handler=_IMPORT_SINGLE_TABLE_
table_num=5

[AN_3d_view_button]
keys=table_num,tooltip,bind_to_list
key_types=_STRING_,_STRING_,_STRING_
table_num=5
bind_to_list=3d_buttons
tooltip=This pops up a 3 dimensional view of the current Boost-Table.  Editing of the table in the 3D view is supported with the +/- and PgUp/PgDn keys (fine/coarse edit) and the arrows navigate. A Red dot shows the current edit position as does the indicators on the right side of the window beneath the buttons.  A left mouse button holddown and drag of the mouse in the 3D view will rotate the display.  Using the middle mouse button will strafe (slide) and the right mouse button will zoom.

[AN_rescale_button]
keys=handler,data
key_types=_ENUM_,_STRING_
handler=_RESCALE_TABLE_
data=AN_AN_rescale_entry,AN_AN_rescale_combo

#----- Rescale Entries ------------------------------------------------------

[AN_rescale_entry]
keys=table_num
key_types=_STRING_
table_num=5

#----- Rescale Combos ------------------------------------------------------

[AN_rescale_combo]
keys=table_num,post_functions_with_arg
key_types=_STRING_,_STRING_
table_num=5
post_functions_with_arg=lock_entry

#----- MAIN VETable Container ------------------------------------------------

[AN_main_table]
keys=rescaler_frame
key_types=_STRING_
rescaler_frame=AN_AN_rescale_frame


# MSnS-E Wheel Decoder

[global]
description=Object data map for MSnS-Extra Wheel Decoder
tab_name=_Wheel Decoder
id_prefix=WD_
groups=triggers,trig_ret,oddfire

#----- Groups ----------------------------------------------------------------

[triggers]
keys=page,base,raw_lower,raw_upper,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_ENUM_,_ENUM_,_STRING_
page=0
base=10
raw_lower=0
raw_upper=255
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=The Trigger points are the points on the crank wheel for triggering each ignition event.  You should set the trigger points at least 20 deg before your MAXIMUM timing advance. (i.e if you set max advance to 40 deg, then the tooth you trigger on should be crossing the sensor at about 60deg BTDC).  See http://megasquirt.sf.net/extra/setup-wheel.html for more information regarding the tooth wheel decoder.

[trig_ret]
keys=page,base,raw_lower,raw_upper,bind_to_list,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_STRING_,_ENUM_,_ENUM_,_STRING_
page=0
base=10
raw_lower=0
raw_upper=255
dl_type=_IMMEDIATE_
handler=_GENERIC_
bind_to_list=trigger_return
tooltip=The trigger return points are ONLY used for cranking if you select "Trigger Return" based cranking below. These points (depending on your wheel used) should end up at around 10deg BTDC for easy starting. See http://megasquirt.sf.net/extra/setup-wheel.html for more information regarding the tooth wheel decoder.

[oddfire]
keys=page,base,raw_lower,raw_upper,dl_conv_expr,ul_conv_expr,is_float,precision,dl_type,bind_to_list,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_STRING_,_STRING_,_BOOL_,_INT_,_ENUM_,_STRING_,_ENUM_,_STRING_
page=6
base=10
raw_lower=0
raw_upper=255
dl_conv_expr=x/0.352
ul_conv_expr=x*0.352
is_float=TRUE
precision=2
dl_type=_IMMEDIATE_
handler=_ODDFIRE_ANGLE_
bind_to_list=oddfire_controls
tooltip=The Oddfire bins are HIGHLY EXPERIMENTAL and likely to be buggy. These are degree offsets for each corresponding trigger point to handle oddfire engines. See http://megasquirt.sourceforge.net/extra/setup-oddwheel.html for more information.

